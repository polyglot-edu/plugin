		"eslint": "^8.26.0",
		"process": "^0.11.10",
		"ts-loader": "^9.5.0",
		"typescript": "^5.2.2",
		"webpack": "^5.89.0",
		"webpack-cli": "^5.1.4"
	},
	"dependencies": {
		"axios": "^1.5.1",
		"git": "^0.1.5",
		"helmet": "^7.1.0",
		"node-fetch": "^3.3.2",
		"path-browserify": "^1.0.1"
	}
