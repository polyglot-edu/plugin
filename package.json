{
	"name": "polyglot",
	"displayName": "polyglot",
	"description": "polyglot",
	"version": "0.0.1",
	"publisher": "Polyglot",
	"engines": {
		"vscode": "^1.74.0"
	},
	"files": [
		"media",
		"README.md",
		"CHANGELOG.md"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/SaraFerrari99/webview_tirocinio.git",
		"logo": "https://i.postimg.cc/gjCQ9yVB/Bell.jpg"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
